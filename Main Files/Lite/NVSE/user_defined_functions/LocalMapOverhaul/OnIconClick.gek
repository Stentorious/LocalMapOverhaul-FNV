int	iMenuID
int	iTileID
string_var sUIPath

ref rIcon

begin Function {iMenuID, iTileID, sUIPath}

	; Clickable icon functionality
	sUIPath = GetActiveUIComponentFullName
	if GetUIFloatAlt (sUIPath + "\_ShowName") == 1
		rIcon = StringToRef (GetUIString (sUIPath + "\_RefID"))
	endif

	sv_Destruct sUIPath

end